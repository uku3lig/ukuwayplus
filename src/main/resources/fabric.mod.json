{
  "schemaVersion": 1,
  "id": "ukuway-plus",
  "version": "${version}",
  "name": "ukuway+",
  "description": "feature-packed qol mod for the hideaway server",
  "authors": [
    "uku",
    "the hideaway+ team"
  ],
  "contact": {
    "sources": "https://github.com/uku3lig/ukuwayplus"
  },
  "license": "MIT",
  "icon": "assets/ukuwayplus/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.uku3lig.ukuway.UkuwayPlus"
    ],
    "ukulib": [
      "net.uku3lig.ukuway.config.UkulibHook"
    ]
  },
  "accessWidener": "ukuwayplus.accesswidener",
  "mixins": [
    "ukuwayplus.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13",
    "minecraft": "1.20.1",
    "ukulib": "^0.7.0",
    "fabric-key-binding-api-v1": "*",
    "fabric-lifecycle-events-v1": "*"
  },
  "breaks": {
    "optifabric": "*"
  }
}
